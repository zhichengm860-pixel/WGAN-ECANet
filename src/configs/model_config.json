{
  "model": {
    "name": "EnhancedWGANECANet",
    "num_classes": 24,
    "input_channels": 2,
    "input_length": 1024
  },
  "architecture": {
    "multi_scale": {
      "enabled": true,
      "kernel_sizes": [1, 3, 5, 7],
      "num_filters": 64
    },
    "eca_attention": {
      "enabled": true,
      "gamma": 2,
      "b": 1
    },
    "spectral_norm": {
      "enabled": true
    },
    "residual": {
      "enabled": false
    }
  },
  "generator": {
    "input_dim": 100,
    "output_dim": 2048,
    "hidden_dims": [512, 1024]
  },
  "discriminator": {
    "input_dim": 2048,
    "hidden_dims": [1024, 512, 256],
    "output_dim": 1
  },
  "classifier": {
    "input_dim": 2048,
    "hidden_dims": [1024, 512, 256],
    "num_classes": 24,
    "dropout": 0.3
  },
  "training": {
    "batch_size": 256,
    "learning_rate": 0.000125,
    "weight_decay": 0.0005,
    "gradient_penalty": 10.0,
    "num_epochs": 60,
    "discriminator_steps": 5,
    "generator_steps": 1
  },
  "data": {
    "signal_length": 1024,
    "num_channels": 2,
    "normalization": "minmax",
    "spectral_normalization": true
  }
}
